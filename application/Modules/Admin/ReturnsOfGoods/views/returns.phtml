namespace JetApplicationModule\Admin\ReturnsOfGoods;


use Jet\Tr;
use JetApplication\Admin_Managers;
use Jet\MVC_View;
use JetApplication\ReturnOfGoods;

/**
 * @var MVC_View $this
 * @var ReturnOfGoods[] $returns
 */
$returns = $this->getRaw('returns');
?>

<h5><?=Tr::_('Return of goods')?></h5>
<table>
	<?php foreach( $returns as $return ): ?>
		<tr>
			<td><?=Admin_Managers::ReturnOfGoods()->renderItemName( $return->getId() )?></td>
			<td><?=$return->getStatus()->showAdmin()?></td>
		</tr>
	<?php endforeach; ?>
</table>
