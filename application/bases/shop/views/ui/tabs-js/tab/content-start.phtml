<?php

use Jet\UI_tabsJS_tab;
use Jet\MVC_View;

/**
 * @var MVC_View $this
 * @var UI_tabsJS_tab $t
 */
$t = $this->getRaw( 'element' );

if($t->getIsSelected()) {
	$t->setBaseCssClass('tab-pane active');
} else {
	$t->setBaseCssClass('tab-pane');
}

$t->setId($GLOBALS['__current_js_tabs'].'_'.$t->getId());
?>
<div <?=$t->renderTagAttributes()?>>

