<?php

use Jet\UI_tabs_tab;
use Jet\MVC_View;

/**
 * @var MVC_View $this
 * @var UI_tabs_tab $t
 */
$t = $this->getRaw( 'element' );

if($t->getIsSelected()) {
	$t->setBaseCssClass('tabs-tab active');
} else {
	$t->setBaseCssClass('tabs-tab');
}
?>
<li><a <?=$t->renderTagAttributes()?>><?= $t->getTitle() ?></a></li>
